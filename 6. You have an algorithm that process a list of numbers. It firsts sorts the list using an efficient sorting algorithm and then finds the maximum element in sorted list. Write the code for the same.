def process_numbers(numbers):
    sorted_numbers = sorted(numbers)
    max_element = sorted_numbers[-1] if sorted_numbers else None  
    return max_element
numbers = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
max_number = process_numbers(numbers)
print(f"The maximum number in the sorted list is: {max_number}")
